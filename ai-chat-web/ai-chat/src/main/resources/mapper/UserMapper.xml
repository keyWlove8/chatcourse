<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.k8.mapper.UserMapper">

    <select id="selectById" parameterType="string" resultType="com.k8.entity.UserInfo">
        SELECT * FROM user_info WHERE user_id = #{userId}
    </select>

    <select id="selectByUserName" parameterType="string" resultType="com.k8.entity.UserInfo">
        SELECT * FROM user_info WHERE username = #{userName}
    </select>

</mapper>


