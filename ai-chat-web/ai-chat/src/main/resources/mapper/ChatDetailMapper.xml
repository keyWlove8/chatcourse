<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.k8.mapper.ChatDetailMapper">

    <select id="getChatDetailByMemoryId" parameterType="string" resultType="com.k8.entity.ChatDetail">
        SELECT * FROM chat_detail WHERE memory_id = #{memoryId}
    </select>

    <delete id="deleteByMemoryId" parameterType="string">
        DELETE FROM chat_detail WHERE memory_id = #{memoryId}
    </delete>

</mapper>
